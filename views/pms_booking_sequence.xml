    <record id="sequence_invoice" model="ir.sequence">
        <field name="name">PMS Invoice Sequence</field>
        <field name="code">pms_invoice_sequence</field>
        <field name="prefix">INVC</field>
        <field name="padding">5</field>
    </record>


    <record id="sequence_transaction" model="ir.sequence">
        <field name="name">PMS Transaction Sequence</field>
        <field name="code">pms_transaction_sequence</field>
        <field name="prefix">TRA</field>
        <field name="padding">5</field>
    </record>


    <record id="sequence_sale_order" model="ir.sequence">
        <field name="name">PMS Sale Order Sequence</field>
        <field name="code">pms_sale_order_sequence</field>
        <field name="prefix">SL</field>
        <field name="padding">5</field>
    </record>

