<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="sequence_booking" model="ir.sequence">
        <field name="name">PMS Booking Sequence</field>
        <field name="code">pms_booking_sequence</field>
        <field name="prefix">B</field>
        <field name="padding">5</field>
    </record>


    <record id="sequence_delivery" model="ir.sequence">
        <field name="name">PMS Delivery Sequence</field>
        <field name="code">pms_delivery_sequence</field>
        <field name="prefix">D</field>
        <field name="padding">5</field>
